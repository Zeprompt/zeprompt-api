NODE_ENV=development

# DATABASE CONFIGURATION
PGHOST=your_db_host
PGDATABASE=your_database
PGUSER=your_user
PGPASSWORD=your_password
PGPORT=5432
PGSSLMODE=require
PGCHANNELBINDING=require

# JWT CONFIGURATION
JWT_EXPIRES_IN=24h
JWT_SECRET=your_jwt_secret_key_here

# MAILZEET CONFIGURATION
MAILZEET_API_KEY=your_mailzeet_api_key
MAILZEET_FROM_EMAIL=notification@yourdomain.com
MAILZEET_FROM_NAME=YourAppName

# REDIS CONFIGURATION
REDIS_USERNAME=default
REDIS_PASSWORD=your_redis_password
REDIS_HOST=your_redis_host
REDIS_PORT=6379

# FRONTEND CONFIG
FRONTEND_URL=http://localhost:5173

# CLOUDFLARE R2 CONFIGURATION
# Obtenir ces valeurs depuis: https://dash.cloudflare.com/ > R2
CLOUDFLARE_BUCKET_NAME=your-bucket-name
CLOUDFLARE_ENDPOINT_URL=https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
CLOUDFLARE_ACCESS_KEY_ID=your_r2_access_key_id
CLOUDFLARE_SECRET_ACCESS_KEY=your_r2_secret_access_key

# URL publique du bucket (optionnel - si vous avez configuré un domaine custom)
# Exemple: https://cdn.yourdomain.com
# Si non configuré, l'endpoint R2 sera utilisé
CLOUDFLARE_PUBLIC_URL=

# PORT
PORT=3000
